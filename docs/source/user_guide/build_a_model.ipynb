  {
   "cell_type": "code",
   "execution_count": null,
   "id": "31",
   "metadata": {},
   "outputs": [],
   "source": [
    "# plot example of nodes\n",
    "dt = 10.0\n",
    "accumulated_time = 0.0\n",
