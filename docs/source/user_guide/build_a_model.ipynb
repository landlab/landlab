    "\n",
    "# update the outlet node:\n",
    "mg.status_at_node[outlet_id] = mg.BC_LINK_IS_FIXED\n",
    "np.any(mg.status_at_link == mg.BC_LINK_IS_FIXED)\n",
    "\n",
